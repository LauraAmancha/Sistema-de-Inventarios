@model IEnumerable<SistemasInventarios.Models.Proveedor>

<h2>Proveedores</h2>

<a href="/Proveedores/Create">Agregar Proveedor</a>

<table border="1">
    <tr>
        <th>Nombre</th>
        <th>Dirección</th>
        <th>Teléfono</th>
        <th>Email</th>
    </tr>
    @foreach (var pr in Model)
    {
        <tr>
            <td>@pr.Nombre</td>
            <td>@pr.Direccion</td>
            <td>@pr.Telefono</td>
            <td>@pr.Email</td>
        </tr>
    }
</table>

